{
  "expo": {
    "name": "Tap Royale",
    "slug": "tap-royale-new",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "tap",
    "platforms": ["ios", "android"],
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "packagerOpts": {
      "assetExts": ["ttf", "otf", "png", "jpg"]
    },
    "ios": {
      "bundleIdentifier": "com.royale.tap",
      "supportsTablet": false,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.royale.tap",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-android.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-5341386294719747~3697991876",
          "iosAppId": "ca-app-pub-5341386294719747~6060431334"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "1df914e9-8a7e-4810-8f0c-3a7ce18de5e4"
      }
    }
  }
}
